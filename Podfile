platform :ios, '10.0'

target 'Genny' do
    use_frameworks!
    inhibit_all_warnings!

    pod 'SwiftyJSON'
    pod 'GDChannel'
    pod 'BlueSocket'
    pod 'SwiftKeychainWrapper'
    
    # fixed versions
    pod 'PKHUD', '~> 4.0'
    pod 'Alamofire', '~> 4.0'
    pod 'PromiseKit', '~> 4.0'
end
